<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <dataProviders>
      <json>
        <disableGroup>publishing</disableGroup>
      </json>
    </dataProviders>
		<events>
			<event name="publish:end">
				<handler type="Sitecore.Publishing.HtmlCacheClearer, Sitecore.Kernel">
					<patch:delete />
				</handler>
			</event>
      <event name="publish:end:remote">
        <handler type="Sitecore.Publishing.HtmlCacheClearer, Sitecore.Kernel" method="ClearCache">
          <patch:delete />
				</handler>
			</event>
		</events>
    <databases>
      <database id="web">
        <patch:delete />
      </database>
		</databases>
		<scheduling>				
			<agent type="Sitecore.Tasks.CleanupFDAObsoleteMediaData">
        <patch:delete />
			</agent>
      <agent type="Sitecore.Tasks.PublishAgent">
				<patch:delete />
			</agent>				
		</scheduling>
		<sites>
			<site name="modules_website" set:database="master" />
			<site name="website" set:database="master" set:filterItems="true" set:enableWorkflow="true" />
		</sites>
    <contentSearch>
      <indexConfigurations>
        <indexUpdateStrategies>
          <onPublishEndAsync>
            <patch:delete />
          </onPublishEndAsync>
          <onPublishEndAsyncSingleInstance>
            <patch:delete />
          </onPublishEndAsyncSingleInstance>
        </indexUpdateStrategies>
      </indexConfigurations>
      <configuration>
        <indexes>
          <index id="sitecore_web_index">
            <patch:delete />
          </index>
          <index id="sitecore_fxm_web_index">
            <strategies>
              <strategy set:ref="contentSearch/indexConfigurations/indexUpdateStrategies/syncMaster" />
            </strategies>
            <locations>
              <crawler>
                <Database>master</Database>
              </crawler>
            </locations>
          </index>
          <index id="social_messages_web">
            <strategies>
              <strategy set:ref="contentSearch/indexConfigurations/indexUpdateStrategies/syncMaster" />
            </strategies>
            <locations>
              <crawler>
                <Database>master</Database>
              </crawler>
            </locations>
          </index>
          <index id="sitecore_marketing_asset_index_web">
            <strategies>
              <strategy set:ref="contentSearch/indexConfigurations/indexUpdateStrategies/syncMaster" />
            </strategies>
            <locations>
              <crawler>
                <Database>master</Database>
              </crawler>
            </locations>
          </index>
          <index id="sitecore_marketingdefinitions_web">
            <strategies>
              <strategy set:ref="contentSearch/indexConfigurations/indexUpdateStrategies/syncMaster" />
            </strategies>
            <locations>
              <crawler>
                <Database>master</Database>
              </crawler>
            </locations>
          </index>
        </indexes>
      </configuration>
    </contentSearch>
  </sitecore>
</configuration>